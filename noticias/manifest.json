{
  "name": "Noticias Aldeadavila",
  "version": "1.0",
  "manifest_version": 2,
  "description": "Noticias sobre aldeadavila.",
  "browser_action": {
    "default_icon": {
            "19": "img/icono.png",
            "38": "img/icono.png"
        },
    "default_popup": "noticias.html"
  },
  "icons": {
      "16": "img/icono.png",
      "128": "img/icono.png"
  },
  "permissions": [
    "contextMenus"
  ],
  "background": {
    "scripts": [
    "js/background.js"
    ]
  }
}
